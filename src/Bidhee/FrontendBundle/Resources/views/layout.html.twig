    <script src="{{ asset('bundles/bidheefrontend/js/jquery-1.11.3.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/bidheefrontend/js/bootstrap.min.js') }}" type="text/javascript"></script>

    <script type="text/javascript" src="//platform-api.sharethis.com/js/sharethis.js#property=58e35a819c801d00126fbfac&product=inline-share-buttons"></script>
    {% block title %} <title>Janata Samachar</title> {% endblock %}
</head>

