    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no"/>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    {% block ogTags %}
        <meta property="og:url" content="{{ url('bidhee_frontend_homepage') }}"/>
